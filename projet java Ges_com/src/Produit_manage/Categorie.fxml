<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="500.0" style="-fx-background-color: #c7cf00;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Produit_manage.ControllerCategorie">
   <children>
      <Label layoutX="29.0" layoutY="72.0" prefHeight="18.0" prefWidth="120.0" text="Nom de la Categorie">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font></Label>
      <Button fx:id="valider_bt" layoutX="33.0" layoutY="150.0" mnemonicParsing="false" onAction="#valider" prefHeight="48.0" prefWidth="111.0" text="Ajouter">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
      <TextField fx:id="nomCat_txt" layoutX="21.0" layoutY="106.0" />
      <TableView fx:id="categorie_table" layoutX="186.0" layoutY="37.0" onMouseClicked="#tableClick" prefHeight="188.0" prefWidth="306.0">
        <columns>
          <TableColumn fx:id="idCat_column" prefWidth="120.0" text="IDENTIFIANT" />
          <TableColumn fx:id="nomCat_column" minWidth="0.0" prefWidth="197.0" text="NOM CATEGORIE" />
        </columns>
      </TableView>
      <Button fx:id="modifier_bt" layoutX="217.0" layoutY="239.0" mnemonicParsing="false" onAction="#modifier" prefHeight="48.0" prefWidth="127.0" text="Modifier">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
      <Button fx:id="supprimer_bt" layoutX="357.0" layoutY="239.0" mnemonicParsing="false" onAction="#supprimer" prefHeight="48.0" prefWidth="136.0" text="Supprimer">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
      <Label fx:id="params_lb" layoutX="10.0" layoutY="6.0" prefHeight="25.0" prefWidth="294.0" text="Categorie">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="precedent_cat" layoutX="14.0" layoutY="248.0" mnemonicParsing="false" onAction="#retour_cat" prefHeight="30.0" prefWidth="111.0" text="Retour" textFill="#f02816">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
